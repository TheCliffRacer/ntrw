 {
  "manifest_version": 2,
  "name": "Random Anime Wallpaper",
  "version": "1.0",

  "description": "New Tab Random (Anime) Wallpaper",

  "icons": {
    "48": "icons/icon-48.png"
  },

  "permissions": ["tabs", "webRequest", "https://www.reddit.com/*", "https://i.redd.it/*", "storage"],

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "background": {
    "scripts": ["index.js"],
    "persistent": false,
    "type": "module"
  }
}